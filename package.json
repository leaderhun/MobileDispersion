{
  "name": "mobile-dispersion",
  "version": "1.0.0",
  "description": "MobileDispersion",
  "main": "index.js",
  "scripts": {
    "pkg-lock-off": "npm config set package-lock false",
    "clean-node": "del node_modules && del package-lock.json",
    "clean-all": "npm run clean-node && npm run clean-dist",
    "lint": "eslint . --ext .js --ext .jsx --no-eslintrc dist",
    "lint-fix": "eslint . --fix --ext .js --ext .jsx --no-eslintrc dist",
    "i": "npm run pkg-lock-off && npm i",
    "i-force": "npm run clean-node && npm run i",
    "start": "webpack-dev-server --port 8080 --env.API_URL= --config build/webpack.run.config.js --mode development",
    "build": "webpack --env.API_URL= --config build/webpack.prod.config.js --progress --profile --color"
  },
  "author": "Norbert Szoboszlai",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.0.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.1.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "css-loader": "1.0.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "html-webpack-plugin": "3.2.0",
    "postcss-loader": "3.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.1",
    "webpack": "4.20.2",
    "webpack-babel-env-deps": "1.5.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "axios": "0.18.0",
    "bootstrap": "4.3.1",
    "react": "16.8.0",
    "react-bootstrap": "1.0.0-beta.6",
    "react-dom": "16.8.0",
    "react-graph-vis": "1.0.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "vis": "^4.21.0"
  }
